{
    "name": "kunstmaan/article-bundle",
    "type": "symfony-bundle",
    "description": "Add articles to your website. Use the KunstmaanGeneratorBundle to generate a new Overview/Detail article section for your website",
    "keywords": [ "kunstmaan", "cms", "article", "news"],
    "homepage": "https://github.com/Kunstmaan/KunstmaanArticleBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Kunstmaan Team",
            "homepage": "http://www.kunstmaan.be/",
            "email": "support@kunstmaan.be"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": ">=2.2.0,<2.3.0",
        "white-october/pagerfanta-bundle": "dev-master",
        "pagerfanta/pagerfanta": "dev-master",
        "kunstmaan/admin-bundle": "2.2.8.*",
        "kunstmaan/adminlist-bundle": "2.2.8.*",
        "kunstmaan/node-bundle": "2.2.8.*"
    },
    "autoload": {
        "psr-0": { "Kunstmaan\\ArticleBundle": "" }
    },
    "target-dir": "Kunstmaan/ArticleBundle",
    "minimum-stability": "dev"
}
